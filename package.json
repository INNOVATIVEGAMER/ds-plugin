{
  "name": "figma-dtcg-exporter",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:plugin\" \"npm run dev:ui\"",
    "dev:plugin": "vite build --config vite.config.plugin.ts --watch",
    "dev:ui": "vite build --watch",
    "build": "npm run build:plugin && npm run build:ui",
    "build:plugin": "vite build --config vite.config.plugin.ts",
    "build:ui": "vite build"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.98.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-singlefile": "^2.0.0"
  }
}
